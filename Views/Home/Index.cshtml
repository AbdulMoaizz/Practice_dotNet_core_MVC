@model IEnumerable<WebApp.Models.Employee>

<html>
<head>
    <title>
    </title>
</head>
<body>
    <h3>Employee List</h3>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Department</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.Department</td>
                    <td>@employee.Name</td>
                    <td>@employee.Email</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>